<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}ApplicationTalents{% endblock %}</title>
    <link rel="icon" type="image/png" href="{{ asset('images/divers/flavicon.png') }}"/>

    <link href="https://fonts.googleapis.com/css2?family=Architects+Daughter&family=KoHo:wght@200&display=swap"
          rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" rel="stylesheet">


    {# <meta property='og:title' content='Title of the article'/> #}
    {# <meta property='og:image' content='//media.example.com/ 1234567.jpg'/> #}
    {# <meta property='og:description' content='Description that will show in the preview'/> #}
    {# <meta property='og:url' content='//http://localhost/ApplicationTalents/public/result'/> #}

    <link href="{{ asset('css/style.css') }}" rel="stylesheet">
    <link href="{{ asset('css/ratingStars.css') }}" rel="stylesheet">
    <link href="{{ asset('css/swup.css') }}" rel="stylesheet">
    <link href="{{ asset('css/particles.css') }}" rel="stylesheet">
    <link href="{{ asset('css/musicale.css') }}" rel="stylesheet">
    <link href="{{ asset('css/kinesthesique.css') }}" rel="stylesheet">
    <link href="{{ asset('css/dragAndDrop.css') }}" rel="stylesheet">
    <link href="{{ asset('css/sandrine.css') }}" rel="stylesheet">
    {% block stylesheets %}{% endblock %}
</head>

<body>
<header>
    {% include("inc/messageFlash.html.twig") %}
        <a href="{{ path('main') }}"><img src="{{ asset('images/divers/rdvNomadeLogo.png') }}" alt="logo" title="Revenir à l'accueil"></a>
        <h1>A la découverte de vos talents...</h1>
</header>

<main>
    <div id="particles-js"></div>
    <div class="transition-fade" id="swup">
        {% block body %}{% endblock %}
    </div>
</main>

<footer>
    {% include("inc/footer.html.twig") %}
</footer>

<script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
<script src="https://raw.githack.com/eKoopmans/html2pdf/master/dist/html2pdf.bundle.js"></script>
<script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>
<script src="https://unpkg.com/swup@latest/dist/swup.min.js"></script>
<script src="https://cdn.jsdelivr.net/particles.js/2.0.0/particles.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/howler/2.2.1/howler.min.js"></script>

<script src="{{ asset('js/particles.js') }}"></script>
<script src="{{ asset('js/requeteAjax.js') }}"></script>
<script src="{{ asset('js/dragAndDropApp.js') }}"></script>
<script src="{{ asset('js/musicale2.js') }}"></script>
<script src="{{ asset('js/result.js') }}"></script>
<script src="{{ asset('js/app.js') }}"></script>
<script>
    const swup = new Swup();
    init();
    let audioBackground = new Howl({
        src: ['audio/KomikuDeHerbeSousLesPieds.mp3'],
        autoplay: true,
        loop: true,
        volume: 0.0,
    });
    audioBackground.play();
    swup.on('contentReplaced', init);
</script>
</body>
</html>
